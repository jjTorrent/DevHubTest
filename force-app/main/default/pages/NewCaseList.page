<apex:page controller="NewCaseListController">
    <apex:form >
        <apex:pageBlock >
            <apex:repeat value="{!NewCases}" var="case">
                <apex:outputLink value="/{!case.Id}">{!case.CaseNumber}</apex:outputLink><br/>
            </apex:repeat>
        </apex:pageBlock>
    </apex:form>
</apex:page>